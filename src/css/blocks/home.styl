.section--pyrolysis-map
	position relative
	margin-top -230px
	background: rgba(53, 55, 64, .9)
	padding-bottom 56px

.whatis-pyrolysis
	width 100%
	min-height 150px
	padding-left 185px
	display flex
	align-items center
	p
		position relative
		text-transform uppercase
		font-size 50px
		line-height 48px
		color #fff
		font-family: mpb
		padding-left 50px
		&:before
			position absolute
			content ''
			top 0
			left 0
			width 6px
			height 100%
			background: #fff
		span
			display block
			font-size 65px

.pyrolysis-map--wrapper
	position relative
	padding 45px 0
	&:before
		position absolute
		top 0
		left 0
		content ''
		height 100%
		width 75%
		background: #fff
	.row
		position relative

.pyrolysis-map--image
	width 60%
	display flex
	align-items center
	.pyrolysis-image
		padding 10px
		background: #fff
		padding: 12px 15px 0px 30px
		max-width 740px
	img
		max-width 100%

.pyrolysis-map--content
	width 40%

.pyrolysis-content
	display flex
	align-items flex-start
	&:not(:last-child)
		margin-bottom 40px
	
	p
		text-align justify
	
	.number-counter
		position relative
		top -15px
		font-size 46px
		line-height 30px
		min-width 78px
		padding-left 30px
		color #fff
		font-family: mpr
		text-shadow: 1px 0 0 #E87966, -1px 0 0 #E87966, 0 0px 0 #E87966, 0 -1px 0 #E87966, 1px 1px #E87966, -1px -1px 0 #E87966, 1px -1px 0 #E87966, -1px 1px 0 #E87966
		&:before
			position absolute
			left 0
			bottom 0
			content ''
			display block
			width 85px
			height 1px
			background: #E87966

.section-installation
	position relative
	padding 23px 0
	.container
		position relative

.installation-img
	width 100%
	max-width 1000px
	display block
	margin 0 auto

.installation-text
	position absolute
	top 0
	left 0
	width 100%
	text-align center
	margin-top 30px
	h3
		font-size 44px
		line-height 54px
		color #000
		font-family: mpb
		span
			color #E04930
			font-size 72px

.section-consultation
	background: #E04830
	min-height 140px
	padding 20px 0px
	display flex
	align-items center

.consultation-wrapper
	padding-left 235px
	p
		color #fff
		font-family: mpb
		font-size 52px

.section-waste
	position relative
	padding-bottom 30px

.waste-wrapper
	padding-left 210px
	padding-right 60px
	padding-top 55px
	background-image: url(../img/waste-bg.jpg)
	background-repeat no-repeat
	background-size 50%
	background-position: 0 bottom
	
.waste-head
	position relative
	padding-left 26px
	margin-bottom 30px
	&:before
		position absolute
		left 0
		top 0
		content ''
		width 6px
		height 100%
		background: #E04830
	h3
		font-family: mpb
		font-size 60px
		line-height 62px
		color #E04830
.list-waste
	li
		font-size 15px
		line-height 32px
		&:not(:last-child)
			margin-bottom 6px
.waste--images-table
	position relative
	display flex
	flex-direction row
	flex-wrap wrap
	margin -10px
	&:before
		position absolute
		right 0
		bottom 0
		content ''
		width: 0
		height: 0
		border-bottom: 160px solid #18488A
		border-left: 160px solid transparent
	.waste-images
		position relative
		width 50%
		padding 10px
		font-size 0
		img
			width 100%


.section-advantage
	background-image: url(../img/advantage-bg.jpg)
	background-repeat no-repeat
	background-size cover
	padding-top 55px
	padding-bottom 55px

.advantage-wrapper
	padding-left 210px
	padding-right 60px

.advantage-head
	position relative
	margin-bottom 50px
	&:before
		position absolute
		left 0
		top 0
		content ''
		width 6px
		height 100%
		background: #fff
	h3
		font-family: mpb
		font-size 70px
		line-height 54px
		color #fff
		padding-left 20px
		span
			font-size 52px

.advantage-main
	display flex
	flex-direction row
	
.advantage-items
	display flex
	flex-direction column
	align-items center
	min-height 150px
	padding 0 10px
	width 20%
	&:not(:last-child)
		border-right 1px solid #fff
	&--icon
		position relative
		width 50px
		height 50px
		margin-bottom 24px
	img
		width 100%
	
	p
		text-align center
		color #fff
		font-size 12px

.waste-profit--head
	position relative
	min-height 160px
	padding 20px 0
	display flex
	align-items center
	justify-content center
	&:before
		position absolute
		top 100%
		left 50%
		content ''
		border-left: 40px solid transparent
		border-right: 40px solid transparent
		border-top: 40px solid #fff
		tt(-50%, 0)
	h3
		font-family: mpb
		font-size 70px
		color #E04830
		text-align center

.table-profit--main
	padding-top 60px
	padding-bottom 10px
	background-image: url(../img/bg-profit.jpg)
	background-repeat no-repeat
	background-position: center 0

.table-profit--head
	padding-left 50px
	position relative
	margin-bottom 20px
	&:before
		position absolute
		left 0
		top 0
		content ''
		width 6px
		height 100%
		background: #fff
	h4
		font-family: mpb
		font-size 44px
		color #fff

.tablet-main
	position relative
	&:before,
	&:after
		content: ''
		width: 1px
		height: 32px
		display: block
		position: relative
		left: 50%
		margin-left: -1px
		background: #5F5F62
	&:after
		height: 60px
	img
		width 100%

.tablet-main--wrapper
	position absolute
	width 100%
	padding 28px
	table
		width 100%
		background: #F6F8F8
		border-collapse: collapse
		tr
			&:last-child
				td
					border-bottom 0px
			td
				border-bottom 1px solid #BBC7D9
				border-right 1px solid #BBC7D9
				padding 18px 12px
				&:last-child
					border-right 0
				&:first-child
					background: #DE482F
					color #fff

.section--specifications
	background-image: url(../img/tehnick-bg.jpg)
	background-repeat no-repeat
	background-size cover
	padding-top 40px
	padding-bottom 65px
.specifications-bg
	width 100%
.specifications-head
	position relative
	margin-bottom 10px
	&:before
		position absolute
		left 0
		top 0
		content ''
		width 6px
		height 100%
		background: #E04930
	h4
		font-family: mpb
		font-size 50px
		line-height 80px
		color #E04930
		padding-left 30px

.specifications-wrapper
	position relative
	&:before,
	&:after
		content: ''
		width: 1px
		height: 32px
		display: block
		position: relative
		left: 50%
		margin-left: -1px
		background: #5F5F62
	&:after
		top -4px
.specifications-tabl--main
	position absolute
	padding 28px 26px
	width 100%
	

.specifications-tabl
	width 100%
	background: #F6F8F8
	border-collapse: collapse
	tr
		&:last-child
			td
				border-bottom 0px
		td
			border-bottom 1px solid #BBC7D9
			border-right 1px solid #BBC7D9
			padding 26px 12px
			&.col-bg
				background: #E4614B
				color #fff
			&.no-col-bg
				background none !important
				color #000 !important
			&:last-child
				border-right 0
			&:first-child
				background: #DE482F
				color #fff

.section--sertificat
	background: #353740
	.container
		position relative
		padding-top 20px
		padding-bottom 66px
		&:before
			position absolute
			top 0
			right: 10%
			display block
			width 5000px
			min-height 355px
			background: #fff
			content ''
	.default-head
		margin-bottom 20px
.sertificat-wrapper
	position relative
	display flex
	flex-direction row
	margin 0 -5px 20px
.sertificat-items
	width calc(100% / 6)
	padding 0 5px
	img
		max-width 100%

.gallery-border
	position relative
	height 1px
	width 100%
	background: #fff
	&:before
		position relative
		display block
		content ''
		left calc(100% / 6)
		top 1px
		width calc((100% / 6) - 8px)
		height 8px
		background: #E04930

.slider-wrapper
	// background: rgba(53,55,64, .89)
	&:before
		position absolute
		right -68px
		bottom -80px
		content ''
		width: 0
		height: 0
		border-bottom 80px solid #15498B
		border-left 80px solid #C7E2F5
		border-right 80px solid #5998D1
		border-top 80px solid transparent
		transform-origin 50% 50%
		transform rotateZ(45deg)
		z-index 10

// .slider-post--item
// 	position relative
.section-posts
	.container
		background-image: url(../img/posts-bg.jpg)
		background-repeat no-repeat
		background-position: 0 bottom
		background-size 65%
	.default-head
		margin-top 30px
		margin-bottom 30px
.slider-post
	position relative
	background: rgba(53,55,64, .6)

.slider-post--content
	min-height 500px
	display flex
	flex-direction column
	h5
		font-size 28px
		line-height 26px
		color #fff
		min-height 110px
		max-width 290px
		padding 44px 20px 20px
	.slider-post--text
		padding-top 10px
		max-width 370px
		padding 10px 20px 20px
		background: rgba(53,55,64, .89)
		flex: 1
		p
			color #fff
			font-size 14px
			line-height 18px

.slider-wrapper
	position relative

.navigate-post
	position absolute
	bottom 0
	right -28px
	z-index 1000
	.slick-arrow
		width 40px
		height 40px
		position relative
		border 1px solid #333
		cursor pointer
		&:first-child
			border-bottom 0
		&:before
			position absolute
			content ''
			display block
			width 7px
			height 7px
			border-width 0 0 1px 1px
			border-color #000
			border-style solid
	.top-sliser--prev
		&:before
			transform rotateZ(135deg)
			top 18px
			left 14px
	.top-sliser--next
		&:before
			transform rotateZ(-45deg)
			left 14px
			top 12px


.section-contact
	position relative
	background: #344EA1
	padding-top 50px
	padding-bottom 50px
	
.contact-left
	text-align center
.logo-contact
	display block
	img
		max-width 160px
.tel-big
	display inline-block
	margin-top 50px
	font-size 36px
	text-decoration none
	color #fff

.contact-descript
	display flex
	flex-direction column
	justify-content space-between
	height 100%
	p,a
		font-size 24px
		color #fff
		text-decoration none
		margin-bottom 20px
		&:last-child
			margin-bottom 0

.section-map
	iframe
		min-height 400px
		width 100%


.burger-menu
	display none
	flex-direction column
	span
		display inline-block
		width 24px
		height 2px
		margin-bottom 4px
		background: #333
		&:last-child
			margin-bottom 0